{
    "name": "自用插件",
    "version": "0.1.0",
    "description": "详见 README.md",
    "manifest_version": 3,
    "icons": {
        "128": "icons/icon128.png",
        "256": "icons/icon256.png"
    },

    "author": "Linehing",
    "content_scripts": [
        {
            "matches": ["https://juejin.cn/*"],
			"css": ["juejin/juejin.css"],
            "js": ["juejin/juejin.js"],
            "run_at": "document_end"
        },
        {
            "matches": ["https://juejin.cn/pins*"],
            "css": ["juejin/juejin-pins.css"]
        }


    ]
}
