{
    "name": "掘金",
    "version": "1.0.0",
    "manifest_version": 3,

    "author": "Linehing",
    "content_scripts": [
        {
            "matches": ["https://juejin.cn/*"],
            "css": ["css/ad.css"]
        },
        {
            "matches": ["https://juejin.cn/pins*"],
